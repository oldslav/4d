@import "_colors.styl"

.text-overline
  text-transform uppercase !important

.list-item-active
  background-color $primary-light

  .q-icon
    color $primary

  &:before
    content ''
    width 3px
    background $primary
    height 100%
    position absolute
    top 0
    left 0

.list-item-avatar
  min-width unset
